<?xml version='1.0' encoding='utf-8'?>
<widget id="com.liquery.mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LiQuery Mobile</name>
    <description>
        LiQuery Mobile
    </description>
    <author email="contact@liquery.com" href="https://www.liquery.com">
        LiQuery
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
