<?xml version='1.0' encoding='utf-8'?>
<widget id="com.liquery.mobile" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LiQuery Mobile</name>
    <description>
        LiQuery Mobile
    </description>
    <author email="contact@liquery.com" href="https://www.liquery.com">
        LiQuery
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="~2.1.2" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.1" />
    <engine name="ios" spec="~4.1.0" />
    <engine name="android" spec="~5.1.1" />
    <engine name="windows" spec="~4.3.1" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-admobpro" spec="~2.29.23" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
</widget>
